<?xml version="1.0" encoding="utf-8"?>

<configuration>
    <startup>
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>
    <appSettings>
        <add key="allowableStages" value="low,rc,prod"/>
        <add key="fileConfig.base.fileName" value="configs"/>
        <add key="fileConfig.base.ext" value="json"/>
        <add key="showValuesToClient" value="false"/>

        <add key="sd.address.low" value="http://127.0.0.1:8500/" />
        <add key="sd.prefix.low" value="p2/systemName" />
        <add key="sd.token.low" value="" /> <!-- can be empty -->

        <add key="sd.address.rc" value="http://127.0.0.1:8500/" />
        <add key="sd.prefix.rc" value="rc/systemName" />
        <add key="sd.token.rc" value="" /> <!-- can be empty -->

        <add key="sd.address.prod" value="http://127.0.0.1:8500/" />
        <add key="sd.prefix.prod" value="systemName" />
        <add key="sd.token.prod" value="" /> <!-- can be empty --> 
    </appSettings>
</configuration>